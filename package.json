{
  "name": "geoia-backend",
  "version": "1.0.0",
  "description": "GeoIA Backend Server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet",
    "lint:fix": "eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "test:debug": "mocha -r ts-node/register --inspect-brk test/**/*.ts --exit",
    "format": "prettier --check .",
    "scripts:download-mapas": "ts-node -r dotenv/config -T scripts/maps-download.ts",
    "scripts:municipios": "ts-node -r dotenv/config -T scripts/municipios.ts",
    "scripts:queimadas": "ts-node -r dotenv/config -T scripts/queimadas.ts",
    "scripts:prepare": "run-s scripts:download-mapas scripts:municipios scripts:queimadas",
    "prepare": "husky install"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "apicache": "^1.6.3",
    "axios": "^1.2.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.2",
    "chalk": "^4",
    "commander": "^10.0.0",
    "compression": "^1.7.4",
    "consola": "^2.15.3",
    "cookie-parser": "^1.4.6",
    "cron": "^2.2.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.7",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.0",
    "enquirer": "^2.4.1",
    "express": "^4.17.3",
    "express-openapi-validator": "^4.13.8",
    "helmet": "^6.0.1",
    "knex": "^2.4.1",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pino": "^7.8.1",
    "tmp-promise": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/apicache": "^1.6.1",
    "@types/bluebird": "^3.5.38",
    "@types/chai": "^4.3.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cron": "^2.0.0",
    "@types/decompress": "^4.2.4",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^17.0.21",
    "@types/pino": "^7.0.4",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "chai": "^4.3.6",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.5.3",
    "prettier": "^2.5.1",
    "shelljs": "^0.8.5",
    "supertest": "^6.1.6",
    "sync-dotenv": "^2.7.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
